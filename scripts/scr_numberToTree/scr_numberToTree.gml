function scr_numberToTree(number){
	
	switch(number) {
			case 0:
				break
			case 1:
				 return sPot;
				
				
			case 2:
				return sPotSappling;
				
				
			case 3:
				return sTreeS	
			case 4:
				return sTreeM	
			case 5:
				return sTreeL
			case 6:
				return sTreeXL
				
			case 7:
				return sTreeXXL
			case 8:
				return sTreeXXXL
		
				
		}
		
		
		
}